{
  "name": "react-native-overlay-dialog",
  "version": "0.7.2",
  "description": "React Native dialogs library for Android & iOS, especially for multi dialogs overlay.",
  "keywords": [
    "react",
    "react-native",
    "dialog"
  ],
  "main": "lib/index.js",
  "scripts": {
    "lint": "npx tsc --noEmit && npx eslint \"./src/**/*.@(ts|tsx)\" --fix",
    "test": "echo no tests",
    "build": "npx tsc",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib",
    "src",
    "example",
    "commitlint.config.js",
    ".buckconfig",
    ".eslintrc.js",
    "index.tsx",
    "tsconfig.json"
  ],
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "author": "brunoduan <bruno.duan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "react-native-root-siblings": "3.1.303"
  },
  "devDependencies": {
    "@types/react": "^17.0",
    "@types/react-native": "^0.63",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "react-native-typescript-transformer": "^1.2.12",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "np": "^5.2.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
  }
}
